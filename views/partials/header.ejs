<header class="header">
  <div class="header-bar">
    <div class="header_menu">
      <span onclick="toggleNav()">
        <img class="header_menu-image"
            src="   https://cdn-icons-png.flaticon.com/512/2976/2976215.png "
            alt="">
      </span>
    </div>
    <div class="header_logo">
      <a href="/">
        <img class="header_logo-image" src="/img/logo/Logo_No_BG.png"
            alt="Company Logo">
      </a>
    </div>

    <div class="header_options">
      <span class="header_search" onclick="toggleSearch()"><img
            class="header_search-image" src="/img/icons/search.png"
            alt="Search Icon"></span>
      <a class="header_cart" href="">
        <img class="header_cart-image" src="/img/icons/shopping-cart.png"
            alt="Shopping Cart Icon">
      </a>
    </div>
  </div>

  <div class="header-search">
    <input class="header-search_input" type="text" placeholder="Search Here"
        oninput="debouncedSearchBook()">
    <div class="header-search-books">

    </div>
  </div>
</header>